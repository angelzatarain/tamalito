<Window x:Class="Tamalito.IniciarSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tamalito"
        mc:Ignorable="d"
        Title="IniciarSesion" Height="250" Width="380">
    <Grid>
        <Label x:Name="lbUsuario" Content="Usuario" HorizontalAlignment="Left" Margin="48,39,0,0" VerticalAlignment="Top" Width="90"/>
        <Label x:Name="lbContra" Content="Contraseña" HorizontalAlignment="Left" Margin="48,94,0,0" VerticalAlignment="Top" Width="90"/>
        <TextBox x:Name="tbUsuario" HorizontalAlignment="Left" Height="23" Margin="168,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="140"/>
        <Button x:Name="btContinuar" Content="Continuar" HorizontalAlignment="Left" Margin="208,155,0,0" VerticalAlignment="Top" Width="75" Click="BtContinuar_Click"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="63,155,0,0" VerticalAlignment="Top" Width="75" Click="BtRegresar_Click"/>
        <PasswordBox x:Name="pbContra" HorizontalAlignment="Left" Margin="168,94,0,0" VerticalAlignment="Top" Width="140" Height="26"/>

    </Grid>
</Window>
